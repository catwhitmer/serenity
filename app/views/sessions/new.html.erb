<h1>Log In Here!</h1>

<% if flash[:message] %>
    <%= flash[:message] %>
<% end %>

<%= form_for :user do |f| %>
    <%= f.label :username %><br>
    <%= f.text_field :username %><br>

    <%= f.label :password %><br>
    <%= f.password_field :password %><br>

    <%= f.submit "Log In" %><br>
    <% end %>