<h1>Create a new Massage</h1>

<%= form_for @massage do |f| %>
    <%= f.label :category %><br>
    <%= f.select :category, options_for_select(["swedish", "assisted stretch", "hot stone", "deep tissue", "pre natal", "reflexology"]) %><br>

    <%= f.label :length %><br>
    <%= f.select :length, options_for_select(["30 min", "60 min", "90 min"]) %><br>

    <%= f.submit "Log In" %><br>
    <% end %>