<%= form_for @massage do |f| %>
    <%= f.label :category %><br>
    <%= f.select :category, options_for_select(["Swedish Massage", "Assisted Stretch Massage", "Hot Stone Massage", "Deep Tissue Massage", "Pre Natal Massage", "Reflexology Massage"]) %><br>
    <br>
    <%= f.label :length %><br>
    <%= f.select :length, options_for_select(["30 min", "60 min", "90 min"]) %>
    <br>
    <br>
    <%= f.submit "Create Massage" %>
<% end %>